--------------------------------------------------------
--  DDL for Trigger TBL_COMPANIONS_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_COMPANIONS_TRG" 
BEFORE INSERT OR UPDATE ON "SHB"."TBL_COMPANIONS"
FOR EACH ROW
DECLARE
    v_user_id VARCHAR2(50);
BEGIN
    v_user_id := NVL(COALESCE(V('GL_USER_ID'), '1'), '1');

    IF :NEW.ID IS NULL THEN
        SELECT TBL_COMPANIONS_SEQ.NEXTVAL INTO :NEW.ID FROM SYS.DUAL;
    END IF;

    IF INSERTING THEN
        :NEW.CREATED := SYSDATE;
        :NEW.CREATED_BY := NVL(:NEW.CREATED_BY, v_user_id);
        INSERT INTO TBL_COMPANIONS_LOG (
            ID, REQUEST_ID, MEMBER_ID, IS_GUEST, FULL_NAME, RELATIONSHIP, IS_ACTIVE, LOG_TYPE, DESCRIPTION, CREATED, CREATED_BY
        ) VALUES (
            :NEW.ID, :NEW.REQUEST_ID, :NEW.MEMBER_ID, :NEW.IS_GUEST, :NEW.FULL_NAME, :NEW.RELATIONSHIP,
            :NEW.IS_ACTIVE, 'I', :NEW.DESCRIPTION, SYSDATE, :NEW.CREATED_BY
        );
    ELSIF UPDATING THEN
        :NEW.UPDATED := SYSDATE;
        :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, v_user_id);
        INSERT INTO TBL_COMPANIONS_LOG (
            ID, REQUEST_ID, MEMBER_ID, IS_GUEST, FULL_NAME, RELATIONSHIP, IS_ACTIVE, LOG_TYPE, DESCRIPTION, CREATED, CREATED_BY
        ) VALUES (
            :NEW.ID, :NEW.REQUEST_ID, :NEW.MEMBER_ID, :NEW.IS_GUEST, :NEW.FULL_NAME, :NEW.RELATIONSHIP,
            :NEW.IS_ACTIVE, 'U', :NEW.DESCRIPTION, SYSDATE, :NEW.UPDATED_BY
        );
    END IF;
END;
/
ALTER TRIGGER "SHB"."TBL_COMPANIONS_TRG" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TBL_CONDITIONS_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_CONDITIONS_TRG" 
BEFORE INSERT OR UPDATE ON SHB.TBL_CONDITIONS
FOR EACH ROW
BEGIN
    IF INSERTING THEN
        -- Generate ID if null
        IF :NEW.ID IS NULL THEN
            SELECT SHB.TBL_CONDITIONS_SEQ.NEXTVAL INTO :NEW.ID FROM SYS.DUAL;
        END IF;

        -- Set audit fields
        :NEW.CREATED := SYSDATE;
        :NEW.CREATED_BY := NVL(:NEW.CREATED_BY, COALESCE(V('GL_USER_ID'), 'SYSTEM'));
        :NEW.IS_ACTIVE := NVL(:NEW.IS_ACTIVE, 1); -- Set default IS_ACTIVE to 1

        -- Log INSERT operation
        INSERT INTO SHB.TBL_CONDITIONS_LOG (
            ID, TITLE, SERVICE_ID, IS_ACTIVE, CREATED, CREATED_BY,
            UPDATED, UPDATED_BY, DESCRIPTION, CODE, MEASURE_UNIT_ID, DATA_TYPE, LOG_TYPE
        ) VALUES (
            :NEW.ID, :NEW.TITLE, :NEW.SERVICE_ID,
            :NEW.IS_ACTIVE, :NEW.CREATED, :NEW.CREATED_BY, :NEW.UPDATED,
            :NEW.UPDATED_BY, :NEW.DESCRIPTION, :NEW.CODE, :NEW.MEASURE_UNIT_ID,
            :NEW.DATA_TYPE, 'I'
        );

    ELSIF UPDATING THEN
        -- Set audit fields
        :NEW.UPDATED := SYSDATE;
        :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, COALESCE(V('GL_USER_ID'), 'SYSTEM'));

        -- Log UPDATE operation
        INSERT INTO SHB.TBL_CONDITIONS_LOG (
            ID, TITLE, SERVICE_ID, IS_ACTIVE, CREATED, CREATED_BY,
            UPDATED, UPDATED_BY, DESCRIPTION, CODE, MEASURE_UNIT_ID, DATA_TYPE, LOG_TYPE
        ) VALUES (
            :NEW.ID, :NEW.TITLE, :NEW.SERVICE_ID,
            :NEW.IS_ACTIVE, :NEW.CREATED, :NEW.CREATED_BY, :NEW.UPDATED,
            :NEW.UPDATED_BY, :NEW.DESCRIPTION, :NEW.CODE, :NEW.MEASURE_UNIT_ID,
            :NEW.DATA_TYPE, 'U'
        );
    END IF;
END;
/
ALTER TRIGGER "SHB"."TBL_CONDITIONS_TRG" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TBL_CONDITION_DTL_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_CONDITION_DTL_TRG" 
BEFORE INSERT OR UPDATE ON SHB.TBL_CONDITION_DTL
FOR EACH ROW
BEGIN
  IF :NEW.ID IS NULL THEN
    SELECT SHB.TBL_CONDITION_DTL_SEQ.NEXTVAL INTO :NEW.ID FROM SYS.DUAL;
  END IF;

  IF INSERTING THEN
    :NEW.CREATED := SYSDATE;
    :NEW.CREATED_BY := NVL(:NEW.CREATED_BY, COALESCE(V('GL_USER_ID'), 'SYSTEM'));

    INSERT INTO SHB.TBL_CONDITION_DTL_LOG (
        LOG_ID, ID, CONDITION_ID, MEMBER_GROUP_ID, FISCAL_YEAR_ID, IS_ACTIVE,
        CREATED, CREATED_BY, UPDATED, UPDATED_BY, DESCRIPTION, CHAR_VALUE,
        NUM_VALUE, START_DATE, BOOLEAN_VALUE, DAY_LIMIT, TITLE_DTL, END_DATE,
        LOG_TYPE
    ) VALUES (
        SHB.TBL_CONDITION_DTL_LOG_SEQ.NEXTVAL, :NEW.ID, :NEW.CONDITION_ID,
        :NEW.MEMBER_GROUP_ID, :NEW.FISCAL_YEAR_ID, :NEW.IS_ACTIVE, :NEW.CREATED,
        :NEW.CREATED_BY, :NEW.UPDATED, :NEW.UPDATED_BY, :NEW.DESCRIPTION,
        :NEW.CHAR_VALUE, :NEW.NUM_VALUE, :NEW.START_DATE, :NEW.BOOLEAN_VALUE,
        :NEW.DAY_LIMIT, :NEW.TITLE_DTL, :NEW.END_DATE, 'I'
    );

  ELSIF UPDATING THEN
    :NEW.UPDATED := SYSDATE;
    :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, COALESCE(V('GL_USER_ID'), 'SYSTEM'));

    INSERT INTO SHB.TBL_CONDITION_DTL_LOG (
        LOG_ID, ID, CONDITION_ID, MEMBER_GROUP_ID, FISCAL_YEAR_ID, IS_ACTIVE,
        CREATED, CREATED_BY, UPDATED, UPDATED_BY, DESCRIPTION, CHAR_VALUE,
        NUM_VALUE, START_DATE, BOOLEAN_VALUE, DAY_LIMIT, TITLE_DTL, END_DATE,
        LOG_TYPE
    ) VALUES (
        SHB.TBL_CONDITION_DTL_LOG_SEQ.NEXTVAL, :NEW.ID, :NEW.CONDITION_ID,
        :NEW.MEMBER_GROUP_ID, :NEW.FISCAL_YEAR_ID, :NEW.IS_ACTIVE, :NEW.CREATED,
        :NEW.CREATED_BY, :NEW.UPDATED, :NEW.UPDATED_BY, :NEW.DESCRIPTION,
        :NEW.CHAR_VALUE, :NEW.NUM_VALUE, :NEW.START_DATE, :NEW.BOOLEAN_VALUE,
        :NEW.DAY_LIMIT, :NEW.TITLE_DTL, :NEW.END_DATE, 'U'
    );
  END IF;
END;
/

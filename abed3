  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_ORGANIZATION_TRG" 
  BEFORE INSERT OR UPDATE ON TBL_ORGANIZATION
  FOR EACH ROW
BEGIN
  IF :NEW.ID IS NULL THEN
    SELECT TBL_ORGANIZATION_SEQ.NEXTVAL INTO :NEW.ID FROM SYS.DUAL;
  END IF;

  IF INSERTING THEN
    :NEW.CREATED    := SYSDATE;
    :NEW.CREATED_BY := NVL(:NEW.CREATED_BY, COALESCE(V('GL_USER_ID'), 1));

    INSERT INTO TBL_ORGANIZATION_LOG (
        ID, ORG_NAME,ORG_CODE, ADDRESS,PHONE,EMAIL,WEBSITE,MANAGER_NAME,ESTABLISHED_DATE,CREATED,CREATED_BY,IS_ACTIVE, ABBR_NAME,
        ORG_DESC,FAX,ORGAN_TYPE, LOG_TYPE, LOG_DATE,DESCRIPTION
    ) VALUES (
        :NEW.ID, :NEW.ORG_NAME, :NEW.ORG_CODE,:NEW.ADDRESS,:NEW.PHONE,:NEW.EMAIL,:NEW.WEBSITE,:NEW.MANAGER_NAME,:NEW.ESTABLISHED_DATE,
        :NEW.CREATED,:NEW.CREATED_BY,:NEW.IS_ACTIVE,:NEW.ABBR_NAME,:NEW.ORG_DESC,:NEW.FAX,:NEW.ORGAN_TYPE,'I', 
        SYSDATE,:NEW.DESCRIPTION
    );

 
  ELSIF UPDATING THEN
    :NEW.UPDATED    := SYSDATE;
    :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, COALESCE(V('GL_USER_ID'), 1));
    
    INSERT INTO TBL_ORGANIZATION_LOG (
            ID, ORG_NAME,ORG_CODE, ADDRESS,PHONE,EMAIL,WEBSITE,MANAGER_NAME,ESTABLISHED_DATE,CREATED,CREATED_BY,IS_ACTIVE, ABBR_NAME,
        ORG_DESC,FAX,ORGAN_TYPE, LOG_TYPE, LOG_DATE,DESCRIPTION
    ) VALUES (
         :NEW.ID, :NEW.ORG_NAME, :NEW.ORG_CODE,:NEW.ADDRESS,:NEW.PHONE,:NEW.EMAIL,:NEW.WEBSITE,:NEW.MANAGER_NAME,:NEW.ESTABLISHED_DATE,
         :NEW.CREATED,:NEW.CREATED_BY,:NEW.IS_ACTIVE,:NEW.ABBR_NAME,:NEW.ORG_DESC,:NEW.FAX,:NEW.ORGAN_TYPE,'U', 
         SYSDATE,:NEW.DESCRIPTION
    );
  END IF;
END;

/

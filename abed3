--  DDL for Function GET_HASH_ORDER_NUM
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "SHB"."GET_HASH_ORDER_NUM" (order_num in number)
return varchar2 is

v_order_num number;
begin
  select (to_number(order_num)*6) - 999901230055 into v_order_num from dual;
   return v_order_num;
   exception when others then null;
end;

/
--------------------------------------------------------
--  DDL for Function GET_MAX_ORDERID
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "SHB"."GET_MAX_ORDERID" return varchar2 is
  v_new_maxr_code varchar2(10);
begin

  select to_char(max(p.ID) + 1) as maxid
    into v_new_maxr_code
    from shb.tbl_payment p
   where trunc(p.CREATED) = to_char(trunc(sysdate));
  if v_new_maxr_code is null then
    v_new_maxr_code := '1';
  end if;
  return lpad(v_new_maxr_code, 4, '0');
end;

/
--------------------------------------------------------
--  DDL for Function GET_ORGANPAY_BY_NATIONCODE
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "SHB"."GET_ORGANPAY_BY_NATIONCODE" (P_nationcode in varchar2,
                                                          P_year       number)
  return number is
  v_amount number;
  --v_sqlerrm varchar2(2000);
begin
  v_amount := 0;
  select g.amount
    into v_amount
    from SHB.TBL_ORGAN_PAY g
   where g.fiscal_year = P_year
     and g.nation_code = P_nationcode;
  return v_amount;
exception
  when others then
    return 0;
end;

/
--------------------------------------------------------
--  DDL for Function GET_REMAIN_DAY
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "SHB"."GET_REMAIN_DAY" (p_date in date) return varchar2
is
  V_DATE varchar2(10);
  V_YEAR NUMBER;
  V_MONTH NUMBER;
  v_sql_error varchar2(1000);
BEGIN
  select TO_CHAR(p_date,'YYYY-MM-DD','NLS_CALENDAR=PERSIAN') into V_DATE from dual;
  V_YEAR := to_number(substr(V_DATE,0,4));
  V_MONTH := to_number(substr(V_DATE,6,2));
  V_DATE := (Case when V_MONTH = 12 then V_YEAR + 1 else V_YEAR end) || '-' || (Case when V_MONTH + 1 < 10 then '0' else '' end) ||(Case when V_MONTH = 12 then 1 else V_MONTH + 1 end) || '-01';

  return (TRUNC(To_DATE(V_DATE,'YYYY-MM-DD','NLS_CALENDAR=PERSIAN')) - TRUNC(p_date) -1);
  exception when others then
   v_sql_error := SQLERRM;
   return -99999;
end;

/
--------------------------------------------------------
--  DDL for Function GET_SERVICE_TREE_JSON
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "SHB"."GET_SERVICE_TREE_JSON" (p_parent_id IN NUMBER DEFAULT NULL) RETURN CLOB IS
    l_clob CLOB;
    l_temp_clob CLOB;
    l_cursor SYS_REFCURSOR;
    l_id NUMBER;
    l_parent_id NUMBER;
    l_service_title VARCHAR2(200);
    l_srv_level NUMBER;
    l_is_active NUMBER;
    l_has_children NUMBER;
BEGIN
    DBMS_LOB.CREATETEMPORARY(l_clob, TRUE);

    OPEN l_cursor FOR
        SELECT
            id,
            parent_id_main,
            htf.escape_sc(service_title) AS service_title,
            srv_level,
            is_active,
            (SELECT COUNT(*) FROM shb.tbl_service_catalog c WHERE c.parent_id_main = t.id AND c.is_active = 1) AS has_children
        FROM shb.tbl_service_catalog t
        WHERE is_active = 1
        AND (p_parent_id IS NULL AND parent_id_main IS NULL OR parent_id_main = p_parent_id);

    DBMS_LOB.WRITEAPPEND(l_clob, LENGTH('['), '[');
    LOOP
        FETCH l_cursor INTO l_id, l_parent_id, l_service_title, l_srv_level, l_is_active, l_has_children;
        EXIT WHEN l_cursor%NOTFOUND;

        l_temp_clob := '{"id":"' || TO_CHAR(l_id) || '",';
        l_temp_clob := l_temp_clob || '"parent":"' || CASE WHEN l_parent_id IS NULL THEN '#' ELSE TO_CHAR(l_parent_id) END || '",';
        l_temp_clob := l_temp_clob || '"text":"' || l_service_title || '",';
        l_temp_clob := l_temp_clob || '"icon":"' || CASE WHEN l_srv_level = 4 THEN 'fa fa-check-square-o' ELSE 'fa fa-folder-o' END || '",';
        l_temp_clob := l_temp_clob || '"li_attr":' || CASE WHEN l_srv_level = 4 THEN '{"checkbox": true}' ELSE '{"checkbox": false}' END || ',';
        l_temp_clob := l_temp_clob || '"state":' || 
            CASE 
                WHEN l_srv_level = 4 THEN '{"selectable": true, "opened": false}'
                WHEN l_has_children > 0 THEN '{"selectable": false, "opened": false}'
                ELSE '{"selectable": false, "opened": false}'
            END || ',';
        l_temp_clob := l_temp_clob || '"children":' || CASE WHEN l_has_children > 0 THEN 'true' ELSE 'false' END || ',';
        l_temp_clob := l_temp_clob || '"level":' || l_srv_level || '}';

        DBMS_LOB.WRITEAPPEND(l_clob, LENGTH(l_temp_clob), l_temp_clob);
        IF NOT l_cursor%NOTFOUND THEN
            DBMS_LOB.WRITEAPPEND(l_clob, LENGTH(','), ',');
        END IF;
    END LOOP;
    DBMS_LOB.WRITEAPPEND(l_clob, LENGTH(']'), ']');

    CLOSE l_cursor;
    RETURN l_clob;
EXCEPTION
    WHEN OTHERS THEN
        IF l_cursor%ISOPEN THEN
            CLOSE l_cursor;
        END IF;
        DBMS_LOB.FREETEMPORARY(l_clob);
        RAISE_APPLICATION_ERROR(-20001, 'Error generating JSON: ' || SQLERRM);
END get_service_tree_json;

/
--------------------------------------------------------
--  DDL for Function GET_SHEBA_FORMAT_MASK
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "SHB"."GET_SHEBA_FORMAT_MASK" (p_input in varchar2)
  return number is
  v_result number;
begin
    IF REGEXP_LIKE(p_input, '^IR\d{2}-\d{4}-\d{4}-\d{4}-\d{4}-\d{4}-\d{2}$') THEN
       v_result:=1;
    ELSE
       v_result:=0;
    END IF;
  return v_result;
end;

/
--------------------------------------------------------
--  DDL for Function GET_USER_PAY_PERCENT
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "SHB"."GET_USER_PAY_PERCENT" (p_group_id in number ,p_fiscal_year  in number)
  return number is
  v_percent number;
begin
  v_percent := 0;
  select sum(PERCENT)   into v_percent
    from SHB.TBL_SHB_MEMBERSHIP_PERCENT f
   where IS_USER = 0     and is_active = 1
      and code_group_ID = p_group_id and fiscal_year=p_fiscal_year
   --     and MEMBERSHIP_PAY_ID = p_group_id
 --  group by MEMBERSHIP_PAY_ID;
    group by code_group_ID;
  return 100 - v_percent;
exception
  when others then
    return 100;
end;

/
--------------------------------------------------------
--  DDL for Function JSON_OUTPUT
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "SHB"."JSON_OUTPUT" (
    P_ELIGIBILITY NUMBER,
    P_TOTAL_AMOUNT NUMBER,
    P_MEMBER_AMOUNT NUMBER,
    P_ORG_AMOUNT NUMBER,
    P_REASON VARCHAR2,
    P_DEBUG VARCHAR2
) RETURN VARCHAR2 IS
BEGIN
    RETURN '{
        "action": "eligibility",
        "eligibility": ' || P_ELIGIBILITY || ',
        "total_amount": ' || P_TOTAL_AMOUNT || ',
        "member_amount": ' || P_MEMBER_AMOUNT || ',
        "org_amount": ' || P_ORG_AMOUNT || ',
        "total_possible_amount": ' || P_TOTAL_AMOUNT || ',
        "reason": "' || P_REASON || '",
        "debug": "' || P_DEBUG || '"
    }';
END JSON_OUTPUT;

/
--------------------------------------------------------
--  DDL for Function SHB_CHECK_CLIENT_USER
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "SHB"."SHB_CHECK_CLIENT_USER" (P_username in varchar2,
                                          P_password varchar2)
  return number is
  v_check number;
begin
  v_check := 0;
  select 1
    into v_check
    from mums.tbl_mums_client_users cu
   where cu.username = P_username
     and cu.is_active = 1
     and cu.account_locked = 0
     and cu.password = P_password;
  return v_check;
exception
  when others then
    return 0;
end;

/
--------------------------------------------------------
--  DDL for Function SHB_CHECK_USER
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "SHB"."SHB_CHECK_USER" (P_username in varchar2,
                                              P_password varchar2)
  return number is
  v_check number;
begin
  select 1
    into v_check
    from mums.tbl_mums_user u
   where u.user_name = P_username
     and u.is_active = 1
     and u.account_locked = 0
     and u.clear_password = P_password;
  return v_check;
exception
  when others then
    return 0;
end;

/

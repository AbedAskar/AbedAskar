         ORG_ID,
         DESCRIPTION,
         FISCAL_YEAR,
         CODE_GROUP_ID,
         LOG_TYPE

  )
 VALUES (
      :NEW.ID,
      :NEW.MEMBERSHIP_PAY_ID,
      :NEW.CREATED,
      :NEW.CREATED_BY,
      :NEW.IS_ACTIVE,
      :NEW.IS_USER,
      :NEW.PERCENT,
      :NEW.PERCENT_TITLE,
      :NEW.ORG_ID,
      :NEW.DESCRIPTION,
      :NEW.FISCAL_YEAR,
      :NEW.CODE_GROUP_ID,
      'I'
    );

  ELSIF UPDATING THEN
    :NEW.UPDATED    := SYSDATE;
    :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, COALESCE(V('GL_USER_ID'), 1));

    INSERT INTO TBL_SHB_MEMBERSHIP_PERCENT_LOG
    (
         ID,
         MEMBERSHIP_PAY_ID,
         CREATED,
         CREATED_BY,
         IS_ACTIVE,
         IS_USER,
         PERCENT,
         PERCENT_TITLE,
         ORG_ID,
         DESCRIPTION,
         FISCAL_YEAR,
         CODE_GROUP_ID,
         LOG_TYPE



    ) VALUES (
      :NEW.ID,
      :NEW.MEMBERSHIP_PAY_ID,
      :NEW.CREATED,
      :NEW.CREATED_BY,
      :NEW.IS_ACTIVE,
      :NEW.IS_USER,
      :NEW.PERCENT,
      :NEW.PERCENT_TITLE,
      :NEW.ORG_ID,
      :NEW.DESCRIPTION,
      :NEW.FISCAL_YEAR,
      :NEW.CODE_GROUP_ID,
      'U'
    );
  END IF;
END;


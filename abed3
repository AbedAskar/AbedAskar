--------------------------------------------------------
--  DDL for Type MEMBER_INTEREST_ROW
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TYPE "SHB"."MEMBER_INTEREST_ROW" AS OBJECT (
    action                 VARCHAR2(30),
    eligibility            NUMBER,
    total_amount           NUMBER,
    member_amount          NUMBER,
    org_amount             NUMBER,
    total_possible_amount  NUMBER,
    total_interest         NUMBER,
    year                   VARCHAR2(10),
    yearly_interest        NUMBER,
    interest_rate          NUMBER,
    membership_amount      NUMBER,
    org_contribution_percent NUMBER,
    reason                 VARCHAR2(200),
    debug                  VARCHAR2(1000)
);

CREATE OR REPLACE TYPE shb.member_interest_tbl AS TABLE OF shb.member_interest_row;


--  DDL for Trigger TBL_BANKS_LOG_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_BANKS_LOG_TRG" 
    before insert or update on TBL_BANKS_LOG
    for each row
begin
    if :new.ID is null then
        select TBL_BANKS_LOG_seq.nextval into :new.ID from sys.dual;
    end if;
    if inserting then
        :new.created := sysdate;
        :new.created_by := nvl(sys_context('APEX$SESSION','APP_USER'),user);
    end if;
end;

/

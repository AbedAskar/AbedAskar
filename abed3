--  DDL for Trigger TBL_SETTLEMENT_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_SETTLEMENT_TRG" 
    before insert or update on SHB.TBL_SETTLEMENT
    for each row
begin
    if :new.ID is null then
        select SHB.TBL_SETTLEMENT_SEQ.nextval into :new.ID from sys.dual;
    end if;
      if inserting then
        :new.created    := sysdate;
        :new.created_by := coalesce(v('GL_USER_ID'), 1);
      elsif updating then
        :new.updated    := sysdate;
        :new.updated_by := coalesce(v('GL_USER_ID'), 1);
      end if;
end;

/

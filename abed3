--------------------------------------------------------
--  DDL for Trigger TBL_IPG_ACCOUNT_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_IPG_ACCOUNT_TRG" 
  before insert or update on TBL_IPG_ACCOUNT
  for each row
begin
  if :new.ID is null then
    select "TBL_IPG_ACCOUNT_SEQ".nextval into :new.ID from sys.dual;
  end if;
  if inserting then
    :new.created        := sysdate;
    :new.created_by     := coalesce(v('GL_USER_ID'), 1);
  elsif updating then
    :new.updated    := sysdate;
    :new.updated_by := coalesce(v('GL_USER_ID'), 1);
  end if;

end;

/
ALTER TRIGGER "SHB"."TBL_IPG_ACCOUNT_TRG" ENABLE;

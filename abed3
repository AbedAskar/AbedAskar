  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_AGREEMENT_ALLOCATIONS_TRG" 
BEFORE INSERT OR UPDATE ON SHB.TBL_AGREEMENT_ALLOCATIONS
FOR EACH ROW

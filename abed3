--------------------------------------------------------
--  DDL for Trigger TBL_JOBS_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_JOBS_TRG" 
  before insert or update on TBL_JOBS
  for each row
begin
  -- insert base table
  if :new.ID is null then
  select TBL_JOBS_seq.nextval into :new.ID from sys.dual;
  end if;
  if inserting then
    :new.created    := sysdate;
    :new.created_by  := nvl(:new.created_by,coalesce(v('GL_USER_ID'), 1));

    --insert into history when base table inserting
   insert into TBL_JOBS_log
          (JOB_TITLE,
           CODE,
           IS_ACTIVE,
           PARENT_ID,
           LOAN_AMOUNT,
           log_type,
           LOAN_END_DATE)
    values
          (:new.JOB_TITLE,
           :new.CODE,
           :new.IS_ACTIVE,
           :new.PARENT_ID,
           :new.LOAN_AMOUNT,
           'I',
           :new.LOAN_END_DATE);
           elsif updating then
    :new.updated    := sysdate;
    :new.updated_by := nvl(:new.updated_by,coalesce(v('GL_USER_ID'),  1));

       --insert into history when base table updating
  insert into TBL_JOBS_log
          (JOB_TITLE,
           CODE,
           IS_ACTIVE,
           PARENT_ID,
           LOAN_AMOUNT,
           log_type,
           LOAN_END_DATE)
    values
          (:new.JOB_TITLE,
           :new.CODE,
           :new.IS_ACTIVE,
           :new.PARENT_ID,
           :new.LOAN_AMOUNT,
           'U',
           :new.LOAN_END_DATE);
  end if;
end;

/
ALTER TRIGGER "SHB"."TBL_JOBS_TRG" ENABLE;

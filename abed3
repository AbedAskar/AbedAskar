  IF INSERTING THEN
    :NEW.CREATED    := SYSDATE;
    :NEW.CREATED_BY := NVL(:NEW.CREATED_BY, COALESCE(V('GL_USER_ID'), 1));

    INSERT INTO TBL_IMPORT_XLSX_HISTORY_LOG (
        ID, RECORD_COUNT, IMPORT_DATE, XLSX_TYPE, CREATED, CREATED_BY, IS_ACTIVE,DESCRIPTION,
        IMPORT_SERIAL,OK_RECORD_CNT, FAILED_RECORD_CNT, LOG_TYPE
    ) VALUES (
        :NEW.ID, :NEW.RECORD_COUNT, :NEW.IMPORT_DATE, :NEW.XLSX_TYPE, :NEW.CREATED, :NEW.CREATED_BY, :NEW.IS_ACTIVE, :NEW.DESCRIPTION,
        :NEW.IMPORT_SERIAL, :NEW.OK_RECORD_CNT , :NEW.FAILED_RECORD_CNT, 'I'
    );

  ELSIF UPDATING THEN
    :NEW.UPDATED    := SYSDATE;
    :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, COALESCE(V('GL_USER_ID'), 1));

    INSERT INTO TBL_IMPORT_XLSX_HISTORY_LOG (
        ID, RECORD_COUNT, IMPORT_DATE, XLSX_TYPE, CREATED, CREATED_BY, IS_ACTIVE,DESCRIPTION,
        IMPORT_SERIAL,OK_RECORD_CNT, FAILED_RECORD_CNT, LOG_TYPE
     ) VALUES (
        :NEW.ID, :NEW.RECORD_COUNT, :NEW.IMPORT_DATE, :NEW.XLSX_TYPE, :NEW.CREATED, :NEW.CREATED_BY, :NEW.IS_ACTIVE, :NEW.DESCRIPTION,
        :NEW.IMPORT_SERIAL, :NEW.OK_RECORD_CNT , :NEW.FAILED_RECORD_CNT, 'U'
    );
  END IF;
END;


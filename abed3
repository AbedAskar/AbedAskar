  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_ATTACHMENT_TRG1" 
  before insert or update on TBL_ATTACHMENT
  for each row
begin
  if :new.ID is null then
    select "TBL_ATTACHMENT_SEQ".nextval into :new.ID from sys.dual;
  end if;
  if inserting then
    :new.created        := sysdate;
    :new.created_by     := coalesce(v('GL_USER_ID'), 1);
  elsif updating then
    :new.updated    := sysdate;
    :new.updated_by := coalesce(v('GL_USER_ID'), 1);
  end if;

end;

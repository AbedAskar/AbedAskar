--------------------------------------------------------
--  DDL for Table {log_table}
--------------------------------------------------------

  CREATE TABLE "SHB"."{log_table}" 
   (	"LOG_ID" NUMBER GENERATED ALWAYS AS IDENTITY MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE , 
	"ID" NUMBER, 
	"FACTOR_TITLE" VARCHAR2(200 BYTE), 
	"UNIT" VARCHAR2(100 BYTE), 
	"DESCRIPTION" VARCHAR2(1000 BYTE), 
	"IS_ACTIVE" NUMBER(1,0) DEFAULT 1, 
	"CREATED" DATE DEFAULT SYSDATE, 
	"CREATED_BY" NUMBER, 
	"UPDATED" DATE, 
	"UPDATED_BY" NUMBER, 
	"LOG_TYPE" CHAR(1 BYTE)
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  TABLESPACE "SAHAB" ;
--------------------------------------------------------
--  DDL for View DMRS_INSTALLATION
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "SHB"."DMRS_INSTALLATION" ("DMRS_PERSISTENCE_VERSION", "DMRS_REPORTS_VERSION", "CREATED_ON") AS 
  select 1.6 DMRS_Persistence_Version, 1.0 DMRS_Reports_Version, to_timestamp('2025/05/19 13:07:20','YYYY/MM/DD HH24:MI:SS') Created_On from dual with read only
;
--------------------------------------------------------

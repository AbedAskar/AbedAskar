--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "SHB"."GET_FISCAL_YEAR_BY_ID" (p_yrear_id in number)
  return number is
  v_year number;
begin
  v_year := 0;
select y.title into v_year from  shb.tbl_fiscal_year y where p_yrear_id=y.id;
  return v_year;
exception
  when others then
    return 0;
end;


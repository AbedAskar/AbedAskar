--------------------------------------------------------
--  DDL for Procedure CALC_TOURISM_REQUEST_COST
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "SHB"."CALC_TOURISM_REQUEST_COST" (
    p_request_id IN NUMBER,
    p_base_amount IN NUMBER
) AS
    v_person_count NUMBER;
    v_trip_days NUMBER;
    v_room_id NUMBER;
    v_price_per_night NUMBER;
    v_discount_percent NUMBER := 0;
    v_total_cost NUMBER;
BEGIN
    -- دریافت اطلاعات درخواست
    SELECT PERSON_COUNT, TRIP_DAYS, ROOM_ID
    INTO v_person_count, v_trip_days, v_room_id
    FROM SHB.TBL_TOURISM_REQUESTS
    WHERE ID = p_request_id;

    -- دریافت قیمت اتاق
    SELECT PRICE_PER_NIGHT
    INTO v_price_per_night
    FROM SHB.TBL_TOURISM_ROOMS
    WHERE ID = v_room_id;

    -- محاسبه هزینه پایه
    v_total_cost := v_person_count * v_trip_days * v_price_per_night;

    -- اعمال تخفیف
    SELECT NVL(MAX(DISCOUNT_PERCENT), 0)
    INTO v_discount_percent
    FROM SHB.TBL_TOURISM_DISCOUNT_RULES
    WHERE ACCOMMODATION_ID = (
        SELECT ACCOMMODATION_ID 
        FROM SHB.TBL_TOURISM_REQUESTS 
        WHERE ID = p_request_id
    )
    AND (MIN_PERSON_COUNT IS NULL OR v_person_count >= MIN_PERSON_COUNT)
    AND (MIN_TRIP_DAYS IS NULL OR v_trip_days >= MIN_TRIP_DAYS)
    AND IS_ACTIVE = 1;

    v_total_cost := v_total_cost * (1 - v_discount_percent / 100);

    -- افزودن مبلغ پایه
    v_total_cost := v_total_cost + p_base_amount;

    -- به‌روزرسانی درخواست با هزینه کل
    UPDATE SHB.TBL_TOURISM_REQUESTS
    SET TOTAL_COST = v_total_cost
    WHERE ID = p_request_id;

    COMMIT;
EXCEPTION
    WHEN NO_DATA_FOUND THEN
        RAISE_APPLICATION_ERROR(-20001, 'Invalid request or room ID');
    WHEN OTHERS THEN
        RAISE_APPLICATION_ERROR(-20000, 'Error in cost calculation: ' || SQLERRM);
END;

/
--------------------------------------------------------
--  DDL for Procedure PRC_DOWNLOAD_FILE
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "SHB"."PRC_DOWNLOAD_FILE" 
(
    PI_PRIMARY_ID   IN NUMBER,
    PI_VIEW_TYPE    IN VARCHAR2
)
IS
    C_PRIMARY_ID        CONSTANT VARCHAR2(30) := PI_PRIMARY_ID;
    C_VIEW_TYPE         CONSTANT VARCHAR2(30) := PI_VIEW_TYPE;
    V_BLOB_CONTENT  BLOB;
    V_MIME_TYPE VARCHAR2(500);
    V_FILE_NAME VARCHAR2(500);
    V_CHECK_DATA_IN_DB NUMBER;
    V_ERROR_MESSAGE VARCHAR2(2000);
BEGIN
  
--CHECK DATA IN TABLE  
       SELECT COUNT(*)
       INTO V_CHECK_DATA_IN_DB
       FROM DUAL
       WHERE EXISTS(SELECT ID FROM SHB.TBL_CLIENT_ATTACHMENT
                    WHERE IMAGE_BLOB IS NOT NULL
                    AND ID = C_PRIMARY_ID
                   );
 IF V_CHECK_DATA_IN_DB = 1 THEN
    SELECT IMAGE_BLOB,
           FILE_NAME,
           MIME_TYPE
    INTO
         V_BLOB_CONTENT,
         V_FILE_NAME,
         V_MIME_TYPE
    FROM SHB.TBL_CLIENT_ATTACHMENT
    WHERE ID = PI_PRIMARY_ID; 
    
    --SET HTTP HEADER
    SYS.HTP.INIT;
    SYS.OWA_UTIL.MIME_HEADER(V_MIME_TYPE, FALSE);
    
    -- SET LENGTH
    SYS.HTP.P('content-length: ' || DBMS_LOB.GETLENGTH(V_BLOB_CONTENT));
    
    -- SET MIME TYPE
    V_MIME_TYPE := SUBSTR(V_MIME_TYPE, INSTR(V_MIME_TYPE, '/')+1);
    
    -- DOWNLOAD
    IF C_VIEW_TYPE = '1' THEN
    SYS.HTP.P('Content-Disposition: attachment; filename="' || V_FILE_NAME || '"' );
    
    --
    ELSIF C_VIEW_TYPE = '2' THEN
    SYS.HTP.P('Content-Disposition: filename="' || V_FILE_NAME || '"' );
    END IF;
    
    --SEND HEADERS
    SYS.OWA_UTIL.HTTP_HEADER_CLOSE;
    
    --SEND BLOB DATA
    SYS.WPG_DOCLOAD.DOWNLOAD_FILE(V_BLOB_CONTENT);
    --APEX_APPLICATION.STOP_APEX_ENGINE;
    END IF; 
    
    EXCEPTION WHEN APEX_APPLICATION.E_STOP_APEX_ENGINE THEN 
      V_ERROR_MESSAGE := sqlerrm;
      insert into shb.tbl_temp
      (ERROR)
      values
      (V_ERROR_MESSAGE);
      NULL;
    END PRC_DOWNLOAD_FILE;
    

/
--------------------------------------------------------
--  DDL for Procedure PRC_DOWNLOAD_FILE_DB
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "SHB"."PRC_DOWNLOAD_FILE_DB" 
(
    PI_PRIMARY_ID   IN NUMBER,
    PI_VIEW_TYPE    IN VARCHAR2
)
IS
    C_PRIMARY_ID        CONSTANT VARCHAR2(30) := PI_PRIMARY_ID;
    C_VIEW_TYPE         CONSTANT VARCHAR2(30) := PI_VIEW_TYPE;
    V_BLOB_CONTENT  BLOB;
    V_MIME_TYPE VARCHAR2(500);
    V_FILE_NAME VARCHAR2(500);
    V_CHECK_DATA_IN_DB NUMBER;
    V_ERROR_MESSAGE VARCHAR2(2000);
BEGIN
  
--CHECK DATA IN TABLE  
       SELECT COUNT(*)
       INTO V_CHECK_DATA_IN_DB
       FROM DUAL
       WHERE EXISTS(SELECT ID FROM SHB.TBL_CLIENT_ATTACHMENT
                    WHERE IMAGE_BLOB IS NOT NULL
                    AND ID = C_PRIMARY_ID
                   );
 IF V_CHECK_DATA_IN_DB = 1 THEN
    SELECT IMAGE_BLOB,
           FILE_NAME,
           MIME_TYPE
    INTO
         V_BLOB_CONTENT,
         V_FILE_NAME,
         V_MIME_TYPE
    FROM SHB.TBL_CLIENT_ATTACHMENT
    WHERE ID = PI_PRIMARY_ID; 
    
    --SET HTTP HEADER
    SYS.HTP.INIT;
    SYS.OWA_UTIL.MIME_HEADER(V_MIME_TYPE, FALSE);
    
    -- SET LENGTH
    SYS.HTP.P('content-length: ' || DBMS_LOB.GETLENGTH(V_BLOB_CONTENT));
    
    -- SET MIME TYPE
    V_MIME_TYPE := SUBSTR(V_MIME_TYPE, INSTR(V_MIME_TYPE, '/')+1);
    
    -- DOWNLOAD
    IF C_VIEW_TYPE = '1' THEN
    SYS.HTP.P('Content-Disposition: attachment; filename="' || V_FILE_NAME || '"' );
    
    --
    ELSIF C_VIEW_TYPE = '2' THEN
    SYS.HTP.P('Content-Disposition: filename="' || V_FILE_NAME || '"' );
    END IF;
    
    --SEND HEADERS
    SYS.OWA_UTIL.HTTP_HEADER_CLOSE;
    
    --SEND BLOB DATA
    SYS.WPG_DOCLOAD.DOWNLOAD_FILE(V_BLOB_CONTENT);
    APEX_APPLICATION.STOP_APEX_ENGINE;
    END IF; 
    
    EXCEPTION WHEN APEX_APPLICATION.E_STOP_APEX_ENGINE THEN 
      /*V_ERROR_MESSAGE := sqlerrm;
      insert into shb.tbl_temp
      (ERROR)
      values
      (V_ERROR_MESSAGE);*/
      NULL;
    END PRC_DOWNLOAD_FILE_DB;
    

/
--------------------------------------------------------
--  DDL for Procedure PRC_GET_ORDER_INFO
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "SHB"."PRC_GET_ORDER_INFO" (
                            p_orderId   in  varchar2,
                            p_amount    out number,
                            p_description out varchar2) is
begin

  select amountpay,DESCRIPTION into p_amount,p_description
  from TBL_PAYMENT
  where orderId = p_orderId;

  exception when others then
  p_amount := 0;
end;

/
--------------------------------------------------------
--  DDL for Procedure PRC_RAISE_APP_ERROR
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "SHB"."PRC_RAISE_APP_ERROR" (p_error_code IN number) IS
  v_message VARCHAR2(1000);
BEGIN
  SELECT ERROR_MESSAGE
    INTO v_message
    FROM SHB.TBL_APP_ERRORS
   WHERE ERROR_Code = p_error_code
     AND IS_ACTIVE = 1;
  apex_error.add_error(p_message          => v_message,
                       p_display_location => apex_error.c_inline_with_field_and_notif);
  -- RAISE_APPLICATION_ERROR(v_code, v_message);
EXCEPTION
  WHEN NO_DATA_FOUND THEN
    SELECT ERROR_MESSAGE
      INTO v_message
      FROM SHB.TBL_APP_ERRORS
     WHERE id = 0;
    apex_error.add_error(p_message          => v_message,
                         p_display_location => apex_error.c_inline_with_field_and_notif);
END;

/
--------------------------------------------------------
--  DDL for Procedure PRC_UPDATE_INQUIRY_PAYMENT_RESULT
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "SHB"."PRC_UPDATE_INQUIRY_PAYMENT_RESULT" (
                            p_message      in varchar2,
                            p_result       in number,
                            p_refNumber    in number,
                            p_paiDat       in varchar2,
                            p_verifiedAt   in varchar2,
                            p_status       in number,
                            p_amount       in number,
                            p_orderId      in varchar2,
                            p_description  in varchar2,
                            p_cardNumber   in varchar2,
                            p_wage         in number,
                            p_createdAt    in varchar2,
                            p_out_result   out varchar2) is  
begin
  update SHB.TBL_PAYMENT p
  set p.MESSAGE = p_message,
      p.resultcode = p_result,
      p.REFNUMBER = p_refNumber,
      p.trackid = p_paiDat,
      p.verified_date = p_verifiedAt,
      p.status = p_status,
      p.amount = p_amount,
      p.DESCRIPTION = p_description,
      p.CARDNUMBER = p_cardNumber      
  where orderid = p_orderId;
  p_out_result := 'true';
 exception when others then
  p_out_result := 'false';
end;

/
--------------------------------------------------------
--  DDL for Procedure PRC_UPDATE_PAYMENT_RESULT
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "SHB"."PRC_UPDATE_PAYMENT_RESULT" (
                            p_paiDat       in varchar2,
                            p_amount       in number,
                            p_result       in number,
                            p_status       in number,
                            p_refNumber    in number,
                            p_description  in varchar2,
                            p_cardNumber   in varchar2,
                            p_orderId      in varchar2,
                            p_message      in varchar2,
                            p_out_result   out varchar2) is  
begin
  update SHB.TBL_PAYMENT p
  set p.paidat = p_paiDat,
      p.amount = p_amount,
      p.resultcode = p_result,
      p.status = p_status,
      p.REFNUMBER = p_refNumber,
      p.DESCRIPTION = p_description,
      p.CARDNUMBER = p_cardNumber,
      p.MESSAGE = p_message
  where orderid = p_orderId;
  p_out_result := 'true';
 exception when others then
  p_out_result := 'false';
end;

/
--------------------------------------------------------
--  DDL for Procedure PRC_UPDATE_PAYMENT_TRACKID
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "SHB"."PRC_UPDATE_PAYMENT_TRACKID" (
                            p_orderId   in NUMBER,
                            p_trackId   in NUMBER,
                            p_result    out varchar2) is  
begin
  update SHB.TBL_PAYMENT p
  set p.trackid = p_trackId
  where orderid = p_orderId;
  select 'https://gateway.zibal.ir/start/' || to_char(p_trackId) into p_result from dual; 
  --p_result := 'true';
 exception when others then
  p_result := 'false';
end;


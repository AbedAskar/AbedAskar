;
--------------------------------------------------------
--  DDL for View VW_RESIDENCE
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "SHB"."VW_RESIDENCE" ("ID", "RESIDENCE_NAME", "RESIDENCE_TYPE", "STAFF_CNT", "PHONE_NUMBER", "MOBILE_NUMBER", "PROV_ID", "HOTEL_TYPE", "CITY_ID", "ADDRESS", "POSTAL_CODE", "IS_ACTIVE", "DESCRIPTION", "RANK", "HOTEL_FACILITIES") AS 
  SELECT 
    ID,
    RESIDENCE_NAME,
    CASE RESIDENCE_TYPE 
        WHEN 1 THEN 'Hotel'
        WHEN 2 THEN 'Apartment'
        WHEN 3 THEN 'Guest House'
        ELSE 'Unknown'
    END AS RESIDENCE_TYPE,
    STAFF_CNT,
    PHONE_NUMBER,
    MOBILE_NUMBER,
    PROV_ID,
    CASE HOTEL_TYPE
        WHEN 1 THEN 'Luxury'
        WHEN 2 THEN 'Budget'
        WHEN 3 THEN 'Standard'
        ELSE 'Unknown'
    END AS HOTEL_TYPE,
    CITY_ID,
    ADDRESS,
    POSTAL_CODE,
    IS_ACTIVE,
    DESCRIPTION,
    RANK,
    (SELECT LISTAGG(f.SERVICE_ID, ', ') WITHIN GROUP (ORDER BY f.SERVICE_ID)
     FROM SHB.TBL_TOURISM_FACILITIES f
     WHERE f.RESIDENCE_ID = r.ID AND f.IS_ACTIVE = 1) AS HOTEL_FACILITIES
FROM SHB.TBL_RESIDENCE r
WHERE IS_ACTIVE = 1
;
--------------------------------------------------------
--  DDL for View VW_RESIDENCE_RESTAURANT
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "SHB"."VW_RESIDENCE_RESTAURANT" ("ID", "RESTAURANT_NAME", "RESTAURANT_TYPE", "STAFF_CNT", "PHONE_NUMBER", "MOBILE_NUMBER", "START_HOUR", "END_HOUR", "RESIDENCE_ID", "CUISINE_TYPE", "IS_ACTIVE", "DESCRIPTION", "RANK", "RESTAURANT_FACILITIES") AS 
  SELECT 
    r.ID,
    r.NAME AS RESTAURANT_NAME,
    CASE r.RESTAURANT_TYPE 
        WHEN 1 THEN 'Italian'
        WHEN 2 THEN 'Cafe'
        WHEN 3 THEN 'Fine Dining'
        ELSE 'Unknown'
    END AS RESTAURANT_TYPE,
    r.STAFF_CNT,
    r.PHONE_NUMBER,
    r.MOBILE_NUMBER,
    r.START_HOUR,
    r.END_HOUR,
    r.RESIDENCE_ID,
    CASE r.CUISINE_TYPE
        WHEN 1 THEN 'Italian'
        WHEN 2 THEN 'Persian'
        WHEN 3 THEN 'International'
        ELSE 'Unknown'
    END AS CUISINE_TYPE,
    r.IS_ACTIVE,
    r.DESCRIPTION,
    r.RANK,
    (SELECT LISTAGG(s.SERVICE_NAME, ', ') WITHIN GROUP (ORDER BY s.SERVICE_ID)
     FROM SHB.TBL_SERVICES s
     JOIN SHB.TBL_RESIDENCE_RESTAURANT_FACILITIES f ON f.SERVICE_ID = s.SERVICE_ID
     WHERE f.RESTAURANT_ID = r.ID AND f.IS_ACTIVE = 1) AS RESTAURANT_FACILITIES
FROM SHB.TBL_RESIDENCE_RESTAURANT r
WHERE r.IS_ACTIVE = 1
;
--------------------------------------------------------
--  DDL for View VW_RESTAURANT
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "SHB"."VW_RESTAURANT" ("ID", "RESTAURANT_NAME", "RESTAURANT_TYPE", "STAFF_CNT", "PHONE_NUMBER", "MOBILE_NUMBER", "PROV_ID", "CUISINE_TYPE", "CITY_ID", "ADDRESS", "POSTAL_CODE", "IS_ACTIVE", "DESCRIPTION", "RANK", "RESTAURANT_FACILITIES") AS 
  SELECT 
    ID,
    RESTAURANT_NAME,
    CASE RESTAURANT_TYPE 
        WHEN 1 THEN 'Italian'
        WHEN 2 THEN 'Cafe'
        WHEN 3 THEN 'Fine Dining'
        ELSE 'Unknown'
    END AS RESTAURANT_TYPE,
    STAFF_CNT,
    PHONE_NUMBER,
    MOBILE_NUMBER,
    PROV_ID,
    CASE CUISINE_TYPE
        WHEN 1 THEN 'Italian'
        WHEN 2 THEN 'Persian'
        WHEN 3 THEN 'International'
        ELSE 'Unknown'
    END AS CUISINE_TYPE,
    CITY_ID,
    ADDRESS,
    POSTAL_CODE,
    IS_ACTIVE,
    DESCRIPTION,
    RANK,
    (SELECT LISTAGG(f.SERVICE_ID, ', ') WITHIN GROUP (ORDER BY f.SERVICE_ID)
     FROM SHB.TBL_RESTAURANT_FACILITIES f
     WHERE f.RESTAURANT_ID = r.ID AND f.IS_ACTIVE = 1) AS RESTAURANT_FACILITIES
FROM SHB.TBL_RESTAURANT r
WHERE IS_ACTIVE = 1
;

--  DDL for Trigger TBL_CONTRACTS_LOG_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_CONTRACTS_LOG_TRG" 
    before insert or update on tbl_contracts_log
    for each row
begin
    if :new.ID is null then
        select "SHB"."TBL_CONTRACTS_LOG_SEQ".nextval into :new.ID from sys.dual;
    end if;
    if inserting then
        :new.created := sysdate;
        :new.created_by := nvl(sys_context('APEX$SESSION','APP_USER'),user);
    elsif updating then
    :new.updated := sysdate;
    :new.updated_by := nvl(sys_context('APEX$SESSION','APP_USER'),user);
    end if;

end;

/
ALTER TRIGGER "SHB"."TBL_CONTRACTS_LOG_TRG" ENABLE;

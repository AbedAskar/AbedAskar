  IF :NEW.ID IS NULL THEN
    SELECT SHB.TBL_AGREEMENT_ALLOCATIONS_SEQ.NEXTVAL INTO :NEW.ID FROM SYS.DUAL;
  END IF;

  IF INSERTING THEN
    :NEW.CREATED := SYSDATE;
    :NEW.CREATED_BY := NVL(:NEW.CREATED_BY, COALESCE(V('GL_USER_ID'), 'SYSTEM'));

    INSERT INTO SHB.TBL_AGREEMENT_ALLOCATIONS_LOG (
        ID, AGREEMENT_ID, ALLOCATION_ID, ALLOCATION_AMOUNT,
        IS_ACTIVE, FISCAL_YEAR, LOG_TYPE
    ) VALUES (
        :NEW.ID, :NEW.AGREEMENT_ID, :NEW.ALLOCATION_ID, :NEW.ALLOCATION_AMOUNT,
        :NEW.IS_ACTIVE, :NEW.FISCAL_YEAR, 'I'
    );

  ELSIF UPDATING THEN
    :NEW.UPDATED := SYSDATE;
    :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, COALESCE(V('GL_USER_ID'), 'SYSTEM'));

    INSERT INTO SHB.TBL_AGREEMENT_ALLOCATIONS_LOG (
        ID, AGREEMENT_ID, ALLOCATION_ID, ALLOCATION_AMOUNT,
        IS_ACTIVE, FISCAL_YEAR, LOG_TYPE
    ) VALUES (

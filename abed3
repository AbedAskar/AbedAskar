  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_RESIDENCE_RESTAURANT_FAC_TRG" 
BEFORE INSERT OR UPDATE ON SHB.TBL_RESIDENCE_RESTAURANT_FACILITIES
FOR EACH ROW
DECLARE
    v_log_type CHAR(1);
BEGIN
    IF INSERTING THEN
        v_log_type := 'I';
    ELSIF UPDATING THEN
        v_log_type := 'U';
    END IF;

    INSERT INTO SHB.TBL_RESIDENCE_RESTAURANT_FACILITIES_LOG (
        ID, RESTAURANT_ID, SERVICE_ID, IS_ACTIVE, CREATED, CREATED_BY,
        UPDATED, UPDATED_BY, DESCRIPTION, LOG_TYPE, LOG_CREATED, LOG_CREATED_BY
    ) VALUES (
        SHB.TBL_RESIDENCE_RESTAURANT_FAC_LOG_SEQ.NEXTVAL,
        :NEW.RESTAURANT_ID, :NEW.SERVICE_ID, :NEW.IS_ACTIVE, :NEW.CREATED, :NEW.CREATED_BY,
        :NEW.UPDATED, :NEW.UPDATED_BY, :NEW.DESCRIPTION, v_log_type, SYSDATE, USER
    );
END;

/
ALTER TRIGGER "SHB"."TBL_RESIDENCE_RESTAURANT_FAC_TRG" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TBL_RESIDENCE_RESTAURANT_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_RESIDENCE_RESTAURANT_TRG" 
AFTER INSERT OR UPDATE ON SHB.TBL_RESIDENCE_RESTAURANT
FOR EACH ROW
DECLARE
    v_log_id NUMBER;
BEGIN
    SELECT SHB.TBL_RESIDENCE_RESTAURANT_LOG_SEQ.NEXTVAL INTO v_log_id FROM DUAL;
    IF INSERTING THEN
        INSERT INTO SHB.TBL_RESIDENCE_RESTAURANT_LOG (
            ID, NAME, RESTAURANT_TYPE, STAFF_CNT, PHONE_NUMBER, MOBILE_NUMBER,
            START_HOUR, END_HOUR, CUISINE_TYPE, IS_ACTIVE, CREATED, CREATED_BY,
            UPDATED, UPDATED_BY, DESCRIPTION, RANK, RESIDENCE_ID,
            LOG_TYPE, LOG_CREATED, LOG_CREATED_BY
        ) VALUES (
            v_log_id, :NEW.NAME, :NEW.RESTAURANT_TYPE, :NEW.STAFF_CNT,
            :NEW.PHONE_NUMBER, :NEW.MOBILE_NUMBER, :NEW.START_HOUR, :NEW.END_HOUR,
            :NEW.CUISINE_TYPE, :NEW.IS_ACTIVE, :NEW.CREATED, :NEW.CREATED_BY,
            :NEW.UPDATED, :NEW.UPDATED_BY, :NEW.DESCRIPTION, :NEW.RANK,
            :NEW.RESIDENCE_ID, 'I', SYSDATE, USER
        );
    ELSIF UPDATING THEN
        INSERT INTO SHB.TBL_RESIDENCE_RESTAURANT_LOG (
            ID, NAME, RESTAURANT_TYPE, STAFF_CNT, PHONE_NUMBER, MOBILE_NUMBER,
            START_HOUR, END_HOUR, CUISINE_TYPE, IS_ACTIVE, CREATED, CREATED_BY,
            UPDATED, UPDATED_BY, DESCRIPTION, RANK, RESIDENCE_ID,
            LOG_TYPE, LOG_CREATED, LOG_CREATED_BY
        ) VALUES (
            v_log_id, :NEW.NAME, :NEW.RESTAURANT_TYPE, :NEW.STAFF_CNT,
            :NEW.PHONE_NUMBER, :NEW.MOBILE_NUMBER, :NEW.START_HOUR, :NEW.END_HOUR,
            :NEW.CUISINE_TYPE, :NEW.IS_ACTIVE, :NEW.CREATED, :NEW.CREATED_BY,
            :NEW.UPDATED, :NEW.UPDATED_BY, :NEW.DESCRIPTION, :NEW.RANK,
            :NEW.RESIDENCE_ID, 'U', SYSDATE, USER
        );
    END IF;
END;

/
ALTER TRIGGER "SHB"."TBL_RESIDENCE_RESTAURANT_TRG" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TBL_RESIDENCE_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_RESIDENCE_TRG" 
BEFORE INSERT OR UPDATE ON SHB.TBL_RESIDENCE
FOR EACH ROW
DECLARE
    v_user_id VARCHAR2(50);
BEGIN
    v_user_id := SUBSTR(NVL(V('APP_USER'), USER), 1, 50);

    IF :NEW.ID IS NULL THEN
        SELECT SHB.TBL_RESIDENCE_SEQ.NEXTVAL INTO :NEW.ID FROM DUAL;
    END IF;

    IF INSERTING THEN
        :NEW.CREATED := SYSDATE;
        :NEW.CREATED_BY := NVL(:NEW.CREATED_BY, v_user_id);
        INSERT INTO SHB.TBL_RESIDENCE_LOG (
            ID, RESIDENCE_NAME, RESIDENCE_TYPE, STAFF_CNT, PHONE_NUMBER, MOBILE_NUMBER,
            PROV_ID, CITY_ID, ADDRESS, POSTAL_CODE, CREATED, CREATED_BY, UPDATED, UPDATED_BY,
            IS_ACTIVE, DESCRIPTION, RANK, HOTEL_TYPE, LOG_TYPE, LOG_CREATED, LOG_CREATED_BY
        ) VALUES (
            SHB.TBL_RESIDENCE_LOG_SEQ.NEXTVAL,
            :NEW.RESIDENCE_NAME, :NEW.RESIDENCE_TYPE, :NEW.STAFF_CNT, :NEW.PHONE_NUMBER, :NEW.MOBILE_NUMBER,
            :NEW.PROV_ID, :NEW.CITY_ID, :NEW.ADDRESS, :NEW.POSTAL_CODE, :NEW.CREATED, :NEW.CREATED_BY,
            :NEW.UPDATED, :NEW.UPDATED_BY, :NEW.IS_ACTIVE, :NEW.DESCRIPTION, :NEW.RANK, :NEW.HOTEL_TYPE,
            'I', SYSDATE, v_user_id
        );
    ELSIF UPDATING THEN
        :NEW.UPDATED := SYSDATE;
        :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, v_user_id);
        INSERT INTO SHB.TBL_RESIDENCE_LOG (
            ID, RESIDENCE_NAME, RESIDENCE_TYPE, STAFF_CNT, PHONE_NUMBER, MOBILE_NUMBER,
            PROV_ID, CITY_ID, ADDRESS, POSTAL_CODE, CREATED, CREATED_BY, UPDATED, UPDATED_BY,
            IS_ACTIVE, DESCRIPTION, RANK, HOTEL_TYPE, LOG_TYPE, LOG_CREATED, LOG_CREATED_BY
        ) VALUES (
            SHB.TBL_RESIDENCE_LOG_SEQ.NEXTVAL,
            :NEW.RESIDENCE_NAME, :NEW.RESIDENCE_TYPE, :NEW.STAFF_CNT, :NEW.PHONE_NUMBER, :NEW.MOBILE_NUMBER,
            :NEW.PROV_ID, :NEW.CITY_ID, :NEW.ADDRESS, :NEW.POSTAL_CODE, :NEW.CREATED, :NEW.CREATED_BY,
            :NEW.UPDATED, :NEW.UPDATED_BY, :NEW.IS_ACTIVE, :NEW.DESCRIPTION, :NEW.RANK, :NEW.HOTEL_TYPE,
            'U', SYSDATE, v_user_id
        );
    END IF;
EXCEPTION
    WHEN OTHERS THEN
        RAISE_APPLICATION_ERROR(-20001, 'Trigger error: ' || SQLERRM);
END;

/
ALTER TRIGGER "SHB"."TBL_RESIDENCE_TRG" ENABLE;

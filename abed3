        ID, RULE_TEXT, RULE_TYPE, IS_ACTIVE,
        CREATED, CREATED_BY, LOG_TYPE, LOG_DATE,
        DESCRIPTION, FISCAL_YEAR
    ) VALUES (
        :NEW.ID, :NEW.RULE_TEXT, :NEW.RULE_TYPE, :NEW.IS_ACTIVE,
        :NEW.CREATED, :NEW.CREATED_BY, 'U', SYSDATE,
        :NEW.DESCRIPTION, :NEW.FISCAL_YEAR

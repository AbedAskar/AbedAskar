--  DDL for Trigger TBL_MEASURE_UNIT_BI
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_MEASURE_UNIT_BI" 
BEFORE INSERT OR UPDATE ON SHB.TBL_MEASURE_UNIT
FOR EACH ROW
BEGIN
    IF :NEW.ID IS NULL THEN
        SELECT SHB.TBL_MEASURE_UNIT_SEQ.NEXTVAL INTO :NEW.ID FROM SYS.DUAL;
    END IF;

    IF INSERTING THEN
        :NEW.CREATED := SYSDATE;
        :NEW.CREATED_BY := NVL(:NEW.CREATED_BY, COALESCE(V('GL_USER_ID'), 'SYSTEM'));

        INSERT INTO SHB.TBL_MEASURE_UNIT_LOG (
            LOG_ID,
            ID,
            MEASURE_UNIT_NAME,
            DESCRIPTION,
            IS_ACTIVE,
            LOG_TYPE,
            LOG_DATE,
            LOG_BY
        ) VALUES (
            SHB.TBL_MEASURE_UNIT_LOG_SEQ.NEXTVAL,
            :NEW.ID,
            :NEW.MEASURE_UNIT_NAME,
            :NEW.DESCRIPTION,
            :NEW.IS_ACTIVE,
            'I',
            SYSDATE,
            COALESCE(V('GL_USER_ID'), 'SYSTEM')
        );
    ELSIF UPDATING THEN
        :NEW.UPDATED := SYSDATE;
        :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, COALESCE(V('GL_USER_ID'), 'SYSTEM'));


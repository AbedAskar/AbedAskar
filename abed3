--  DDL for Trigger TBL_CONTRACTS_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_CONTRACTS_TRG" 
  before insert or update on tbl_contracts
  for each row
begin
  -- insert base table
  if :new.id is null then
    select "SHB"."TBL_CONTRACTS_SEQ".nextval into :new.id from sys.dual;
  end if;
  if inserting then
    :new.created    := sysdate;
    :new.created_by  := nvl(:new.created_by,coalesce(v('GL_USER_ID'), 1));

    --insert into history when base table inserting
    insert into tbl_contracts_log
  (
          contract_code,
          title,
          contract_type_id,
          agreement_type,
          signing_date,
          expiration_date,
          resource_release_date,
          commission_percent,
          installment_count,
          installment_amount,
          duration_months,
          account_balance,
          bank_id,
          is_active,
          log_type,
          fiscal_year)
values
(
          :new.contract_code,
          :new.title,
          :new.contract_type_id,
          :new.agreement_type,
          :new.signing_date,
          :new.expiration_date,
          :new.resource_release_date,
          :new.commission_percent,
          :new.installment_count,
          :new.installment_amount,
          :new.duration_months,
          :new.account_balance,
          :new.bank_id,
          :new.is_active,
          'I',
          :new.fiscal_year
);
   --update base table
  elsif updating then
    :new.updated    := sysdate;
    :new.updated_by := nvl(:new.updated_by,coalesce(v('GL_USER_ID'),  1));

       --insert into history when base table updating
 insert into tbl_contracts_log
  (
          contract_code,
          title,
          contract_type_id,
          agreement_type,
          signing_date,
          expiration_date,
          resource_release_date,
          commission_percent,
          installment_count,
          installment_amount,
          duration_months,
          account_balance,
          bank_id,
          is_active,
          log_type,
          fiscal_year)
values
(
          :new.contract_code,
          :new.title,
          :new.contract_type_id,
          :new.agreement_type,
          :new.signing_date,
          :new.expiration_date,
          :new.resource_release_date,
          :new.commission_percent,
          :new.installment_count,
          :new.installment_amount,
          :new.duration_months,
          :new.account_balance,
          :new.bank_id,
          :new.is_active,
          'U',
          :new.fiscal_year
);
  end if;
end;


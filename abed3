--  DDL for Trigger TBL_MESSAGE_TYPE_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_MESSAGE_TYPE_TRG" 
  before insert or update on tbl_message_type
  for each row
begin
  -- insert base table
  if :new.ID is null then
    select tbl_message_type_log_seq.nextval into :new.ID from sys.dual;
  end if;
  if inserting then
    :new.created    := sysdate;
    :new.created_by  := nvl(:new.created_by,coalesce(v('GL_USER_ID'), 1));

    --insert into history when base table inserting
   insert into tbl_message_type_log
  ( title, type_code, is_active,  description, log_type,parent_id)
values
      (:new.title,
       :new.type_code,
       :new.is_active,
       :new.description,
       'I',
       :new.parent_id);
   --update base table
  elsif updating then
    :new.updated    := sysdate;
    :new.updated_by := nvl(:new.updated_by,coalesce(v('GL_USER_ID'),  1));

       --insert into history when base table updating
   insert into tbl_message_type_log
  ( title, type_code, is_active,  description, log_type,parent_id)
values
      (:new.title,
       :new.type_code,
       :new.is_active,
       :new.description,
       'U',
       :new.parent_id);
  end if;
end;

/

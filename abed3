--------------------------------------------------------
--  DDL for Trigger TBL_WORKFLOW_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_WORKFLOW_TRG" 
  BEFORE INSERT OR UPDATE ON TBL_WORKFLOW
  FOR EACH ROW
BEGIN
  IF :NEW.ID IS NULL THEN
    SELECT TBL_WORKFLOW_SEQ.NEXTVAL INTO :NEW.ID FROM SYS.DUAL;
  END IF;

  IF INSERTING THEN
    :NEW.CREATED    := SYSDATE;
    :NEW.CREATED_BY := NVL(:NEW.CREATED_BY, COALESCE(V('GL_USER_ID'), 1));

    INSERT INTO TBL_WORKFLOW_LOG (
        ID,STAGE_GROUP,STAGE_CODE,STAGE_NAME,IS_ACTIVE,CREATED,CREATED_BY,LOG_TYPE,LOG_DATE,DESCRIPTION
    ) VALUES (
        :NEW.ID, :NEW.STAGE_GROUP, :NEW.STAGE_CODE,:NEW.STAGE_NAME,:NEW.IS_ACTIVE,:NEW.CREATED,:NEW.CREATED_BY,'I',
        SYSDATE,:NEW.DESCRIPTION
    );

  ELSIF UPDATING THEN
    :NEW.UPDATED    := SYSDATE;
    :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, COALESCE(V('GL_USER_ID'), 1));

    INSERT INTO TBL_WORKFLOW_LOG (
           ID,STAGE_GROUP,STAGE_CODE,STAGE_NAME,IS_ACTIVE,CREATED,CREATED_BY,LOG_TYPE,LOG_DATE,DESCRIPTION
    ) VALUES (
 :NEW.ID, :NEW.STAGE_GROUP, :NEW.STAGE_CODE,:NEW.STAGE_NAME,:NEW.IS_ACTIVE,:NEW.CREATED,:NEW.CREATED_BY,'U',
        SYSDATE,:NEW.DESCRIPTION    );
  END IF;
END;

/
ALTER TRIGGER "SHB"."TBL_WORKFLOW_TRG" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TRG_CONDITIONS_LOG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TRG_CONDITIONS_LOG" 

AFTER INSERT OR UPDATE OR DELETE ON SHB.TBL_CONDITIONS

FOR EACH ROW

BEGIN

  IF INSERTING THEN

    INSERT INTO SHB.TBL_CONDITIONS_LOG (

        ID, TITLE, SERVICE_ID, IS_ACTIVE, CREATED, CREATED_BY, UPDATED, UPDATED_BY, 

        DESCRIPTION, CODE, DATA_TYPE, MEASURE_UNIT_ID, LOG_TYPE

    ) VALUES (

        :NEW.ID, :NEW.TITLE, :NEW.SERVICE_ID, :NEW.IS_ACTIVE, :NEW.CREATED, :NEW.CREATED_BY,

        :NEW.UPDATED, :NEW.UPDATED_BY, :NEW.DESCRIPTION, :NEW.CODE, :NEW.DATA_TYPE, :NEW.MEASURE_UNIT_ID, 'I'

    );

  ELSIF UPDATING THEN

    INSERT INTO SHB.TBL_CONDITIONS_LOG (

        ID, TITLE, SERVICE_ID, IS_ACTIVE, CREATED, CREATED_BY, UPDATED, UPDATED_BY, 

        DESCRIPTION, CODE, DATA_TYPE, MEASURE_UNIT_ID, LOG_TYPE

    ) VALUES (

        :NEW.ID, :NEW.TITLE, :NEW.SERVICE_ID, :NEW.IS_ACTIVE, :NEW.CREATED, :NEW.CREATED_BY,

        :NEW.UPDATED, :NEW.UPDATED_BY, :NEW.DESCRIPTION, :NEW.CODE, :NEW.DATA_TYPE, :NEW.MEASURE_UNIT_ID, 'U'

    );

  ELSIF DELETING THEN

    INSERT INTO SHB.TBL_CONDITIONS_LOG (

        ID, TITLE, SERVICE_ID, IS_ACTIVE, CREATED, CREATED_BY, UPDATED, UPDATED_BY, 

        DESCRIPTION, CODE, DATA_TYPE, MEASURE_UNIT_ID, LOG_TYPE

    ) VALUES (

        :OLD.ID, :OLD.TITLE, :OLD.SERVICE_ID, :OLD.IS_ACTIVE, :OLD.CREATED, :OLD.CREATED_BY,

        :OLD.UPDATED, :OLD.UPDATED_BY, :OLD.DESCRIPTION, :OLD.CODE, :OLD.DATA_TYPE, :OLD.MEASURE_UNIT_ID, 'D'

    );

  END IF;

END;
/
ALTER TRIGGER "SHB"."TRG_CONDITIONS_LOG" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TRG_GENERATE_TRACKING_CODE
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TRG_GENERATE_TRACKING_CODE" 
BEFORE INSERT ON TBL_SHB_FEEDBACK_REQ
FOR EACH ROW
BEGIN
  :NEW.tracking_code := TO_CHAR(SYSDATE, 'YYYYMMDDHH24MISS') || LPAD(DBMS_RANDOM.VALUE(1000, 9999), 4, '0');
END;
/
ALTER TRIGGER "SHB"."TRG_GENERATE_TRACKING_CODE" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TR_CONDITIONS_AUDIT
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TR_CONDITIONS_AUDIT" 
BEFORE INSERT OR UPDATE ON SHB.TBL_CONDITIONS
FOR EACH ROW
DECLARE
    v_user VARCHAR2(50);
    v_operation VARCHAR2(10);
BEGIN
    -- Set user with fallback
    BEGIN
        v_user := COALESCE(V('GL_USER_ID'), SYS_CONTEXT('USERENV', 'SESSION_USER'), 'SYSTEM');
    EXCEPTION
        WHEN OTHERS THEN
            v_user := SYS_CONTEXT('USERENV', 'SESSION_USER');
    END;

    IF INSERTING THEN
        v_operation := 'INSERT';
        -- Generate ID if null
        IF :NEW.ID IS NULL THEN
            SELECT SHB.TBL_CONDITIONS_SEQ.NEXTVAL INTO :NEW.ID FROM SYS.DUAL;
        END IF;

        -- Set audit fields
        :NEW.CREATED := SYSDATE;
        :NEW.CREATED_BY := NVL(:NEW.CREATED_BY, v_user);
        :NEW.IS_ACTIVE := NVL(:NEW.IS_ACTIVE, 1); -- Set default IS_ACTIVE to 1

        -- Log INSERT operation
        INSERT INTO SHB.TBL_CONDITIONS_LOG (
            ID, TITLE, SERVICE_ID, IS_ACTIVE, CREATED, CREATED_BY,
            UPDATED, UPDATED_BY, DESCRIPTION, CODE, MEASURE_UNIT_ID, DATA_TYPE, LOG_TYPE
        ) VALUES (
            :NEW.ID, :NEW.TITLE, :NEW.SERVICE_ID,
            :NEW.IS_ACTIVE, :NEW.CREATED, :NEW.CREATED_BY, :NEW.UPDATED,
            :NEW.UPDATED_BY, :NEW.DESCRIPTION, :NEW.CODE, :NEW.MEASURE_UNIT_ID,
            :NEW.DATA_TYPE, 'I'
        );

    ELSIF UPDATING THEN
        v_operation := 'UPDATE';
        -- Set audit fields
        :NEW.UPDATED := SYSDATE;
        :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, v_user);

        -- Log UPDATE operation
        INSERT INTO SHB.TBL_CONDITIONS_LOG (
            ID, TITLE, SERVICE_ID, IS_ACTIVE, CREATED, CREATED_BY,
            UPDATED, UPDATED_BY, DESCRIPTION, CODE, MEASURE_UNIT_ID, DATA_TYPE, LOG_TYPE
        ) VALUES (
            :NEW.ID, :NEW.TITLE, :NEW.SERVICE_ID,
            :NEW.IS_ACTIVE, :NEW.CREATED, :NEW.CREATED_BY, :NEW.UPDATED,
            :NEW.UPDATED_BY, :NEW.DESCRIPTION, :NEW.CODE, :NEW.MEASURE_UNIT_ID,
            :NEW.DATA_TYPE, 'U'
        );
    END IF;
EXCEPTION
    WHEN OTHERS THEN
        RAISE_APPLICATION_ERROR(-20001, 'Error in TR_CONDITIONS_AUDIT during ' || v_operation || 
            ': ' || SQLERRM || ' (Check TBL_CONDITIONS_SEQ, table permissions, or column definitions)');
END;
/
ALTER TRIGGER "SHB"."TR_CONDITIONS_AUDIT" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TR_CONDITIONS_DELETE
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TR_CONDITIONS_DELETE" 
BEFORE DELETE ON SHB.TBL_CONDITIONS
FOR EACH ROW
DECLARE
    v_user VARCHAR2(50);
BEGIN
    -- Set user with fallback
    BEGIN
        v_user := COALESCE(V('GL_USER_ID'), SYS_CONTEXT('USERENV', 'SESSION_USER'), 'SYSTEM');
    EXCEPTION
        WHEN OTHERS THEN
            v_user := SYS_CONTEXT('USERENV', 'SESSION_USER');
    END;

    INSERT INTO SHB.TBL_CONDITIONS_LOG (
        ID, TITLE, SERVICE_ID, IS_ACTIVE, CREATED, CREATED_BY,
        UPDATED, UPDATED_BY, DESCRIPTION, CODE, MEASURE_UNIT_ID, DATA_TYPE, LOG_TYPE
    ) VALUES (
        :OLD.ID, :OLD.TITLE, :OLD.SERVICE_ID,
        :OLD.IS_ACTIVE, :OLD.CREATED, :OLD.CREATED_BY, :OLD.UPDATED,
        :OLD.UPDATED_BY, :OLD.DESCRIPTION, :OLD.CODE, :OLD.MEASURE_UNIT_ID,
        :OLD.DATA_TYPE, 'D'
    );
EXCEPTION
    WHEN OTHERS THEN
        RAISE_APPLICATION_ERROR(-20002, 'Error in TR_CONDITIONS_DELETE: ' || SQLERRM || 
            ' (Check table permissions or column definitions)');
END;
/
ALTER TRIGGER "SHB"."TR_CONDITIONS_DELETE" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TR_CONDITION_DTL_AUDIT
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TR_CONDITION_DTL_AUDIT" 
BEFORE INSERT OR UPDATE ON SHB.TBL_CONDITION_DTL
FOR EACH ROW
DECLARE
    v_user VARCHAR2(50) := COALESCE(V('GL_USER_ID'), 'SYSTEM');
BEGIN
    IF INSERTING THEN
        -- Generate ID if null
        IF :NEW.ID IS NULL THEN
            SELECT SHB.TBL_CONDITION_DTL_SEQ.NEXTVAL INTO :NEW.ID FROM DUAL;
        END IF;

        -- Set audit fields
        :NEW.CREATED := SYSDATE;
        :NEW.CREATED_BY := NVL(:NEW.CREATED_BY, v_user);

        -- Log INSERT operation
        INSERT INTO SHB.TBL_CONDITION_DTL_LOG (
            LOG_ID, ID, CONDITION_ID, MEMBER_GROUP_ID, FISCAL_YEAR_ID, IS_ACTIVE,
            CREATED, CREATED_BY, UPDATED, UPDATED_BY, DESCRIPTION, CHAR_VALUE,
            NUM_VALUE, START_DATE, BOOLEAN_VALUE, DAY_LIMIT, TITLE_DTL, END_DATE,
            LOG_TYPE
        ) VALUES (
            SHB.TBL_CONDITION_DTL_LOG_SEQ.NEXTVAL, :NEW.ID, :NEW.CONDITION_ID,
            :NEW.MEMBER_GROUP_ID, :NEW.FISCAL_YEAR_ID, :NEW.IS_ACTIVE, :NEW.CREATED,
            :NEW.CREATED_BY, :NEW.UPDATED, :NEW.UPDATED_BY, :NEW.DESCRIPTION,
            :NEW.CHAR_VALUE, :NEW.NUM_VALUE, :NEW.START_DATE, :NEW.BOOLEAN_VALUE,
            :NEW.DAY_LIMIT, :NEW.TITLE_DTL, :NEW.END_DATE, 'I'
        );

    ELSIF UPDATING THEN
        -- Set audit fields
        :NEW.UPDATED := SYSDATE;
        :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, v_user);

        -- Log UPDATE operation
        INSERT INTO SHB.TBL_CONDITION_DTL_LOG (
            LOG_ID, ID, CONDITION_ID, MEMBER_GROUP_ID, FISCAL_YEAR_ID, IS_ACTIVE,
            CREATED, CREATED_BY, UPDATED, UPDATED_BY, DESCRIPTION, CHAR_VALUE,
            NUM_VALUE, START_DATE, BOOLEAN_VALUE, DAY_LIMIT, TITLE_DTL, END_DATE,
            LOG_TYPE
        ) VALUES (
            SHB.TBL_CONDITION_DTL_LOG_SEQ.NEXTVAL, :NEW.ID, :NEW.CONDITION_ID,
            :NEW.MEMBER_GROUP_ID, :NEW.FISCAL_YEAR_ID, :NEW.IS_ACTIVE, :NEW.CREATED,
            :NEW.CREATED_BY, :NEW.UPDATED, :NEW.UPDATED_BY, :NEW.DESCRIPTION,
            :NEW.CHAR_VALUE, :NEW.NUM_VALUE, :NEW.START_DATE, :NEW.BOOLEAN_VALUE,
            :NEW.DAY_LIMIT, :NEW.TITLE_DTL, :NEW.END_DATE, 'U'
        );
    END IF;
END;
/
ALTER TRIGGER "SHB"."TR_CONDITION_DTL_AUDIT" ENABLE;

--------------------------------------------------------
--  DDL for Trigger PAYMENT_BIU
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."PAYMENT_BIU" 
    before insert or update
    on "SHB"."TBL_PAYMENT"
    for each row
begin
    if :new.ID is null then
        select SHB.TBL_PAYMENT_SEQ.nextval into :new.ID from sys.dual;
    end if;
  if inserting then
    :new.created    := sysdate;
    :new.created_by := coalesce(v('GL_USER_ID'), 1);
  elsif updating then
    :new.updated    := sysdate;
    :new.updated_by := coalesce(v('GL_USER_ID'), 1);
  end if;
end;
/
ALTER TRIGGER "SHB"."PAYMENT_BIU" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TBL_AGREEMENTS_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_AGREEMENTS_TRG" 
BEFORE INSERT OR UPDATE ON SHB.TBL_AGREEMENTS
FOR EACH ROW
BEGIN
  IF :NEW.ID IS NULL THEN
    SELECT SHB.TBL_AGREEMENTS_SEQ.NEXTVAL INTO :NEW.ID FROM SYS.DUAL;
  END IF;

  IF INSERTING THEN
    :NEW.CREATED := SYSDATE;
    :NEW.CREATED_BY := NVL(:NEW.CREATED_BY, COALESCE(V('GL_USER_ID'), 'SYSTEM'));

    INSERT INTO SHB.TBL_AGREEMENTS_LOG (
        ID, CODE, TITLE, SERVICE_TYPE,
        START_DATE, END_DATE, IS_ACTIVE,
        CONTRACTOR_ID, EMPLOYER_ID, FISCAL_YEAR,
        PARENT_ID, LOG_TYPE,CREATED,CREATED_BY
    ) VALUES (
        :NEW.ID, :NEW.CODE, :NEW.TITLE, :NEW.SERVICE_TYPE,
        :NEW.START_DATE, :NEW.END_DATE, :NEW.IS_ACTIVE,
        :NEW.CONTRACTOR_ID, :NEW.EMPLOYER_ID, :NEW.FISCAL_YEAR,
        :NEW.PARENT_ID, 'I',:NEW.CREATED,:NEW.CREATED_BY
    );

  ELSIF UPDATING THEN
    :NEW.UPDATED := SYSDATE;
    :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, COALESCE(V('GL_USER_ID'), 'SYSTEM'));

    INSERT INTO SHB.TBL_AGREEMENTS_LOG (
        ID, CODE, TITLE, SERVICE_TYPE,
        START_DATE, END_DATE, IS_ACTIVE,
        CONTRACTOR_ID, EMPLOYER_ID, FISCAL_YEAR,
        PARENT_ID, LOG_TYPE,CREATED,CREATED_BY
    ) VALUES (
        :NEW.ID, :NEW.CODE, :NEW.TITLE, :NEW.SERVICE_TYPE,
        :NEW.START_DATE, :NEW.END_DATE, :NEW.IS_ACTIVE,
        :NEW.CONTRACTOR_ID, :NEW.EMPLOYER_ID, :NEW.FISCAL_YEAR,
        :NEW.PARENT_ID, 'U',:NEW.CREATED, :NEW.CREATED_BY
    );
  END IF;
END;

/
ALTER TRIGGER "SHB"."TBL_AGREEMENTS_TRG" ENABLE;

--------------------------------------------------------
--  DDL for Trigger TBL_CREDIT_CHECKS_TRG
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "SHB"."TBL_CREDIT_CHECKS_TRG" 
BEFORE INSERT OR UPDATE ON SHB.TBL_CREDIT_CHECKS
FOR EACH ROW
DECLARE
  v_user_id NUMBER := COALESCE(V('GL_USER_ID'), 1);  -- مقدار پیش‌فرض برای شناسه کاربر
BEGIN
  IF :NEW.ID IS NULL THEN
    SELECT TBL_CREDIT_CHECKS_SEQ.NEXTVAL INTO :NEW.ID FROM DUAL;
  END IF;

  IF INSERTING THEN
    :NEW.CREATED    := SYSDATE;
    :NEW.CREATED_BY := NVL(:NEW.CREATED_BY, v_user_id);

    INSERT INTO SHB.TBL_CREDIT_CHECKS_LOG (
      ID, REQUEST_ID, CHECK_DATE, RESULT, COMMENTS,
      IS_ACTIVE, CREATED, CREATED_BY, UPDATED, UPDATED_BY,
      CONFIRM_AMOUNT, LOG_TYPE, LOG_DATE
    ) VALUES (
      :NEW.ID, :NEW.REQUEST_ID, :NEW.CHECK_DATE, :NEW.RESULT, :NEW.COMMENTS,
      :NEW.IS_ACTIVE, :NEW.CREATED, :NEW.CREATED_BY, :NEW.UPDATED, :NEW.UPDATED_BY,
      :NEW.CONFIRM_AMOUNT, 'I', SYSDATE
    );

  ELSIF UPDATING THEN
    :NEW.UPDATED    := SYSDATE;
    :NEW.UPDATED_BY := NVL(:NEW.UPDATED_BY, v_user_id);

    -- نسخه قبل از آپدیت (U_OLD)
    INSERT INTO SHB.TBL_CREDIT_CHECKS_LOG (
      ID, REQUEST_ID, CHECK_DATE, RESULT, COMMENTS,
      IS_ACTIVE, CREATED, CREATED_BY, UPDATED, UPDATED_BY,
      CONFIRM_AMOUNT, LOG_TYPE, LOG_DATE
    ) VALUES (
      :OLD.ID, :OLD.REQUEST_ID, :OLD.CHECK_DATE, :OLD.RESULT, :OLD.COMMENTS,
      :OLD.IS_ACTIVE, :OLD.CREATED, :OLD.CREATED_BY, :OLD.UPDATED, :OLD.UPDATED_BY,
      :OLD.CONFIRM_AMOUNT, 'U_OLD', SYSDATE
    );

    -- نسخه بعد از آپدیت (U_NEW)
    INSERT INTO SHB.TBL_CREDIT_CHECKS_LOG (
      ID, REQUEST_ID, CHECK_DATE, RESULT, COMMENTS,
      IS_ACTIVE, CREATED, CREATED_BY, UPDATED, UPDATED_BY,
      CONFIRM_AMOUNT, LOG_TYPE, LOG_DATE
    ) VALUES (
      :NEW.ID, :NEW.REQUEST_ID, :NEW.CHECK_DATE, :NEW.RESULT, :NEW.COMMENTS,
      :NEW.IS_ACTIVE, :NEW.CREATED, :NEW.CREATED_BY, :NEW.UPDATED, :NEW.UPDATED_BY,
      :NEW.CONFIRM_AMOUNT, 'U_NEW', SYSDATE
    );
  END IF;
END;
/
ALTER TRIGGER "SHB"."TBL_CREDIT_CHECKS_TRG" ENABLE;
